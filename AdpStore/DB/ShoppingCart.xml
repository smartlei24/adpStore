<?xml version="1.0" encoding="utf-8" ?>
<DbConfig xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <SqlConfigs>
    <DbSql CommandName="QueryShoppingCartByUserId" Type="Text" ConnectionName="Test">
      <Text>
        <![CDATA[
        SELECT 
            ShoppingId,
            ProductId,
            UserId,
            Indate
        From dbo.ShoppingCart WITH(NOLOCK)
        WHERE UserId=@UserId
      ]]>
      </Text>
    </DbSql>
    <DbSql CommandName="AddANewShopingCart" Type="Text" ConnectionName="Test">
      <Text>
        <![CDATA[
        INSERT INTO dbo.ShoppingCart (ProductId, UserId) VALUES(@ProductId, @UserId)
      ]]>
      </Text>
    </DbSql>
    <DbSql CommandName="DeleteShoppingCartById" Type="Text" ConnectionName="Test">
      <Text>
        <![CDATA[
        DELETE TOP(1) FROM dbo.ShoppingCart WHERE ShoppingId=@ShoppingId
      ]]>
      </Text>
    </DbSql>
    <DbSql CommandName="DeleteAllShoppingCartByUserId" Type="Text" ConnectionName="Test">
      <Text>
        <![CDATA[
        DELETE FROM dbo.ShoppingCart WHERE UserId=@UserId
      ]]>
      </Text>
    </DbSql>
  </SqlConfigs>
</DbConfig>